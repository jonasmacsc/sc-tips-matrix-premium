{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/",               "destination": "/index.html" },
    { "source": "/estrategias",    "destination": "/estrategias.html" },
    { "source": "/matrix-premium", "destination": "/matrix-premium.html" },
    { "source": "/puxadores",      "destination": "/puxadores.html" },
    { "source": "/contato",        "destination": "/contato.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=0, s-maxage=60" }
      ]
    }
  ]
}
